{
  "name": "tiny-sangtae",
  "version": "0.0.2",
  "description": "A state management library written in TypeScript. It can be used with Vanilla JS and React.",
  "keywords": [
    "store",
    "state",
    "state manager",
    "react"
  ],
  "author": "Lee Hyanggi <ad7549@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/2scent/tiny-sangtae"
  },
  "homepage": "https://github.com/2scent/tiny-sangtae#readme",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/tiny-sangtae.umd.cjs",
  "module": "./dist/tiny-sangtae.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/tiny-sangtae.js",
      "require": "./dist/tiny-sangtae.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/node": "^22.1.0",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "9.x",
    "globals": "^15.8.0",
    "prettier": "3.3.3",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.3.4",
    "vite-plugin-dts": "4.0.0-beta.2",
    "vitest": "^2.0.5"
  }
}
